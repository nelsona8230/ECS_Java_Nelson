//This code displays a message of my initials made out of letters 
public class Initials 
{
	public static void main(String[] args)
	{
		System.out.println("     A     N     N ");
		System.out.println("    A A    N N   N ");
		System.out.println("   A   A   N  N  N ");
		System.out.println("  AAAAAAA  N   N N");
		System.out.println(" A       A N     N");
		System.out.println("");
		System.out.println("");
		System.out.println("");
		System.out.println("");
		System.out.println("");
	}

}
